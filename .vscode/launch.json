{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dart",
            "type": "dart",
            "request": "launch",
            "program": "bin/main.dart"
        },
        {
            "name": "store_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "store_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "store_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Flutter Web",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--web-browser-flag",
                "--disable-web-security",
                "--web-browser-flag",
                "--user-data-dir=/tmp/chrome_dev"
            ]
        },
        {
            "name": "Flutter Web (Edge)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "edge",
                "--web-browser-flag=--disable-web-security",
                "--web-browser-flag=--user-data-dir=/tmp/edge_dev"
            ]
        }
    ]
}